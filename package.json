{
  "name": "DavidBot",
  "version": "1.0.6",
  "description": "Sistema gestion procesos servidor para adn 3.0",
  "main": "mapa.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "rm -rf dist  && export GITHUB_TOKEN=5e3ddbcfcdf2994f2ad5bffe22b30136f348a9b5 &&  export GH_TOKEN=5e3ddbcfcdf2994f2ad5bffe22b30136f348a9b5 && build -p always && build --win -p always",
    "distdev": "rm -rf dist && build && build --win"
  },
  "repository": "https://github.com/fredyteheranto/pm2Admin.git",
  "keywords": [
    "Electron",
    "adn",
    "mapa"
  ],
  "build": {
    "appId": "DavidBotv1",
    "publish": [{
      "provider": "github",
      "owner": "fredyteheranto",
      "repo": "pm2Admin"
    }],
    "asar": true,
    "dmg": {
      "contents": [{
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "icon.ico"
    }
  },
  "author": "fredyteheranto",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-builder": "^20.38.3",
    "electron-updater": "^2.10.1"
  },
  "dependencies": {
    "electron-handlebars": "^2.0.0",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.6",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "nodemiral": "^1.1.1",
    "request": "^2.88.0",
    "smalltalk": "^3.2.2",
    "then-request": "^6.0.0",
    "unirest": "^0.5.1"
  }
}